{
  "name": "repair-notifications-client",
  "version": "1.0.0",
  "description": "Client side code for repair notifications",
  "main": "index.html",
  "scripts": {
    "lint": "eslint \"src/**/*\"",
    "format": "prettier --write \"src/**/*.+(ts|tsx|css|scss)\"",
    "build": "webpack",
    "watch": "webpack -w",
    "start": "npm run build && firebase emulators:start --only hosting",
    "deploy": "firebase deploy --only hosting"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrbrianevans/repair-notifications.git"
  },
  "author": "Brian Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrbrianevans/repair-notifications/issues"
  },
  "homepage": "https://github.com/mrbrianevans/repair-notifications#readme",
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "mini-css-extract-plugin": "^1.3.2",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
